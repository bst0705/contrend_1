<table class="table">
  <tbody>
    <tr>
      <td>
        <%= link_to admin_customer_path(@tweet.customer.id) do %>
          <%= attachment_image_tag @tweet.customer, :profile_image_id, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></br>
          <%= @tweet.customer.name %>
        <% end %>
      </td>
      <td>
        <%= link_to admin_tweet_path(@tweet.id) do %>
        <%= @tweet.title %><br>
        <% end %>
      </td>
      <td><%= @tweet.body %></td>
      <td>PV数　<%= @tweet.impressions_count %></td>
      <td>
        <%= link_to "Destroy", admin_tweet_path(@tweet), class: "btn btn-sm btn-danger", data: {confirm: "本当に削除しますか？"}, method: :delete %>
      </td>
    </tr>
  </tbody>
</table>